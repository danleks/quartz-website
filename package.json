{
  "name": "gatsby-starter-default",
  "homepage": "https://danleks.github.io/kwarctex",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "danleks",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.36",
    "gatsby-background-image": "^0.8.15",
    "gatsby-image": "^2.2.30",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-offline": "^3.0.14",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-scroll-reveal": "0.0.4",
    "gatsby-plugin-sharp": "^2.2.36",
    "gatsby-plugin-styled-components": "^3.1.9",
    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-transformer-remark": "^2.6.32",
    "gatsby-transformer-sharp": "^2.3.2",
    "global": "^4.4.0",
    "intersection-observer": "^0.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-inlinesvg": "^1.1.7",
    "react-scrollmagic": "^2.1.1",
    "styled-components": "^4.4.0",
    "typeface-lato": "0.0.75",
    "typeface-nunito-sans": "0.0.72"
  },
  "devDependencies": {
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-stylelint": "^3.2.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^10.0.0-0",
    "prettier": "^1.18.2",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "stylelint-webpack-plugin": "^1.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0 -p 8000",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ]
  }
}
